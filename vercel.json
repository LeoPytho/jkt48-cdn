{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/([a-f0-9]{32}\\.[a-zA-Z0-9]+)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "GITHUB_TOKEN": "@github_token",
    "GITHUB_OWNER": "@github_owner", 
    "GITHUB_REPO": "@github_repo",
    "GITHUB_BRANCH": "main"
  }
}
